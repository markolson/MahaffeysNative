  getInitialState: function() {
    return { userId: null };
  },

  componentDidMount() {
      this.loadUser().done();  
  },

  async loadUser() {
    var value = await AsyncStorage.getItem("@Mahaffeys:userId");
    this.setState({userId: value});
  },
